  <script>
    const defaultConfig = {
      app_title: 'Brug Evaluatie Wizard',
      subtitle: 'Stap voor stap je brug beoordelen'
    };

    let config = { ...defaultConfig };
    let currentStep = 0;
    let studentName = '';
    let scores = {
      design: 0,
      'min-length': 0,
      'max-length': 0,
      width: 0,
      height: 0,
      weight: 0
    };

    function startEvaluation() {
      studentName = document.getElementById('student-name').value.trim();
      if (!studentName) {
        document.getElementById('student-name').style.borderColor = '#ef4444';
        return;
      }

      document.getElementById('welcome-screen').classList.add('hidden');
      document.getElementById('evaluation-screen').classList.remove('hidden');
      document.getElementById('student-name-display').textContent = studentName;
      currentStep = 0;
      showStep(0);
    }

    function showStep(step) {
      const steps = 6;
      currentStep = step;

      // Hide all steps
      for (let i = 1; i <= steps; i++) {
        document.getElementById(`step-${i}`).classList.add('hidden');
      }

      // Show current step
      document.getElementById(`step-${step + 1}`).classList.remove('hidden');

      // Update progress bar
      const progress = ((step + 1) / steps) * 100;
      document.getElementById('progress-bar').style.width = progress + '%';
      document.getElementById('current-step').textContent = step + 1;

      // Update button states
      document.getElementById('prev-btn').disabled = step === 0;
      document.getElementById('prev-btn').style.opacity = step === 0 ? '0.5' : '1';
      
      if (step === steps - 1) {
        document.getElementById('next-btn').textContent = 'Score Berekenen ‚úì';
      } else {
        document.getElementById('next-btn').textContent = 'Volgende ‚Üí';
      }

      // Clear selections
      document.querySelectorAll('.option-btn').forEach(btn => {
        btn.classList.remove('selected');
        btn.style.borderColor = '';
      });
    }

    function setDesignScore(score) {
      scores.design = score;
      const buttons = document.querySelectorAll('#step-1 .option-btn');
      buttons.forEach(btn => btn.classList.remove('selected'));
      event.target.closest('.option-btn').classList.add('selected');
    }

    function setCriteria(criterion, passed) {
      scores[criterion] = passed ? 1 : 0;
      const buttons = document.querySelectorAll(`#step-${getStepNumber(criterion)} .option-btn`);
      buttons.forEach(btn => btn.classList.remove('selected'));
      event.target.closest('.option-btn').classList.add('selected');
    }

    function getStepNumber(criterion) {
      const mapping = {
        'min-length': 2,
        'max-length': 3,
        'width': 4,
        'height': 5
      };
      return mapping[criterion];
    }

    function updateWeightDisplay(value) {
      document.getElementById('weight-display').textContent = value + ' g';
      
      let weightScore = 0;
      if (value >= 1000) weightScore = 4;
      else if (value >= 750) weightScore = 3;
      else if (value >= 500) weightScore = 2;
      else if (value >= 250) weightScore = 1;
      
      scores.weight = weightScore;
    }

    function nextStep() {
      if (currentStep < 5) {
        showStep(currentStep + 1);
      } else {
        showResults();
      }
    }

    function previousStep() {
      if (currentStep > 0) {
        showStep(currentStep - 1);
      }
    }

    function showResults() {
      document.getElementById('evaluation-screen').classList.add('hidden');
      document.getElementById('results-screen').classList.remove('hidden');

      // Display results
      document.getElementById('student-name-result').textContent = studentName;
      document.getElementById('design-result').textContent = scores.design + '/2';
      document.getElementById('min-length-result').textContent = scores['min-length'] + '/1';
      document.getElementById('max-length-result').textContent = scores['max-length'] + '/1';
      document.getElementById('width-result').textContent = scores.width + '/1';
      document.getElementById('height-result').textContent = scores.height + '/1';
      document.getElementById('weight-result').textContent = scores.weight + '/4';

      const total = scores.design + scores['min-length'] + scores['max-length'] + scores.width + scores.height + scores.weight;
      document.getElementById('total-score').textContent = total;

      // Color code results
      const resultElements = [
        { id: 'design-result', score: scores.design, max: 2 },
        { id: 'min-length-result', score: scores['min-length'], max: 1 },
        { id: 'max-length-result', score: scores['max-length'], max: 1 },
        { id: 'width-result', score: scores.width, max: 1 },
        { id: 'height-result', score: scores.height, max: 1 },
        { id: 'weight-result', score: scores.weight, max: 4 }
      ];

      resultElements.forEach(el => {
        const elem = document.getElementById(el.id);
        if (el.score === el.max) {
          elem.className = 'font-bold text-emerald-400';
        } else if (el.score === 0) {
          elem.className = 'font-bold text-red-400';
        } else {
          elem.className = 'font-bold text-amber-400';
        }
      });

      // Animate celebration
      document.getElementById('celebration').classList.add('celebrate');
    }

    function resetAll() {
      document.getElementById('welcome-screen').classList.remove('hidden');
      document.getElementById('results-screen').classList.add('hidden');
      document.getElementById('student-name').value = '';
      document.getElementById('weight-slider').value = 0;
      scores = {
        design: 0,
        'min-length': 0,
        'max-length': 0,
        width: 0,
        height: 0,
        weight: 0
      };
    }

    // Element SDK Integration
    async function onConfigChange(cfg) {
      document.getElementById('app-title').textContent = cfg.app_title || defaultConfig.app_title;
      document.getElementById('subtitle').textContent = cfg.subtitle || defaultConfig.subtitle;
    }

    function mapToCapabilities(cfg) {
      return {
        recolorables: [],
        borderables: [],
        fontEditable: undefined,
        fontSizeable: undefined
      };
    }

    function mapToEditPanelValues(cfg) {
      return new Map([
        ['app_title', cfg.app_title || defaultConfig.app_title],
        ['subtitle', cfg.subtitle || defaultConfig.subtitle]
      ]);
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities,
        mapToEditPanelValues
      });
    }
  </script>
